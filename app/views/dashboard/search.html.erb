<div class="row search-result">
	<% @athletes.each do |athlete| %>
		<div class="col-md-10 col-md-offset-2 col-sm-10 col-sm-offset-2">
			<div class="well member">
				<%= athlete.full_name %>
			</div>
		</div>
	<% end %>
</div>

<%= will_paginate @athletes, renderer: BootstrapPagination::Rails %>
